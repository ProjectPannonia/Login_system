<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainPage.MainscreenController">
   <children>
      <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="370.0" prefWidth="517.0">
         <children>
            <Label layoutX="7.0" layoutY="8.0" prefHeight="45.0" prefWidth="135.0" text="Main Screen">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label id="logged_in" fx:id="logged_in" alignment="CENTER" layoutX="7.0" layoutY="59.0" prefHeight="45.0" prefWidth="291.0">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ButtonBar layoutX="7.0" layoutY="104.0" prefHeight="140.0" prefWidth="504.0">
              <buttons>
                <Button id="settings_bt" fx:id="settings_bt" mnemonicParsing="false" onAction="#settings" text="Adatok megadása">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button id="logOut_bt" fx:id="logOut_bt" onAction="#logOut" mnemonicParsing="false" prefHeight="25.0" prefWidth="195.0" text="Kijelentkezés">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
              </buttons>
            </ButtonBar>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
